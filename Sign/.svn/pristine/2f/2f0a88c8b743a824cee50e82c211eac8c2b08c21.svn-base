@charset "utf-8";
/* CSS Document */

body{
	width:100%;
	height:100%;
}
#OutBox{
	height:367px;
	width:652px;
	position:absolute;
	left:50%;
	margin-top:-183px;
	margin-left:-326px;
	top:50%;
}
#OutBox .Top{
	background:url(../images/nctop.gif) center top no-repeat;
	height:65px;
}
#OutBox .Mid{
	background:url(../images/mid.gif) center top no-repeat;
	height:263px;
}
#OutBox .Mid .Right{
	padding:80px 70px 0px 50px;
	float:right;
	width:400px;
	float:right;
	height:183px;
}


}
#OutBox .Mid .Right table td{
	color:#333333;
	text-align:left;
	font-size:12px;
	color:#333333;
	line-height:22px;
	padding:5px;
	vertical-align:middle;
}

#OutBox .Mid .Right table td .btnSubmit{
	background:url(../images/btnSubmit.gif) center center no-repeat;
	width:112px;
	height:35px;
	border:0px;
	vertical-align:middle;
	cursor:hand;
}

#OutBox .Bot{
	background:url(../images/bot.gif) center top no-repeat;
	height:38px;
	line-height:38px;
	text-align:center;
	color:#afe6f9;
	font-size:12px;
}

td img,td input { vertical-align:middle;}

